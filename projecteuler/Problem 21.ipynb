    "def sum(n):\n",
    "    count=0\n",
    "    for i in range(1,int(n/2+1)):\n",
    "        if n%i==0:\n",
    "            count+=i\n",
    "    return count\n",
    "value=0\n",
    "for i in range(1,10001):\n",
    "    sum1=sum(i)\n",
    "    if(sum(sum1)==i and i!=sum1):\n",
    "        value+=i\n",
    "print(value)"
