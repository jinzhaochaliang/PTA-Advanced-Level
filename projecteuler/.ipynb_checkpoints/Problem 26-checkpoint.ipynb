    "def recu_cycle(d):\n",
    "    for t in range(1,d):\n",
    "        if 1 == 10**t % d:\n",
    "            return t;\n",
    "    return 0\n",
    "longest = max(recu_cycle(i) for i in range(2,1001))\n",
    "print(longest)\n",
    "print ([i for i in range(2,1001) if recu_cycle(i) == longest][0])"
   ]
